$('.flashMessages').html('<%= j render 'shared/flash_messages' %>')
<% if flash[:danger].present? %>
  $('.commits-list').html('')
<% else %>
  $('.errors-list').html('')
  $('.commits-list').html('<%= j render 'commits', commits: @commits, owner: @owner, repo: @repo %>')
  $('#owner').val('<%= @owner %>')
  $('#repo').val('<%= @repo %>')
<% end %>
